package com.hexaware.java8examples.practicequestion;

import java.util.stream.Stream;

public class Seller {

	public static void main(String[] args) {
	Stream<Integer>	return1 = addSeller();
		displaySeller(return1);
	}
	
	public static Stream<Integer> addSeller() {
		Stream<Integer> sellerID = Stream.of(1,2,3,4,5,6,7,8,9);
		return sellerID;
	}
	public static void displaySeller(Stream<Integer> sellerID)
	{
		sellerID.forEach(seller -> System.out.println(seller));
	}

}
